{
  "useTabs": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "printWidth": 100,
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "proseWrap": "always",
  "bracketSameLine": false,
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
